{
  "version": 2,
  "projects": [
    {
      "name": "fan-london-frontend",
      "src": "frontend",
      "builds": [{ "src": "frontend/package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }],
      "routes": [{ "src": "/(.*)", "dest": "/index.html" }]
    },
    {
      "name": "fan-london-backend",
      "src": "backend",
      "builds": [{ "src": "backend/package.json", "use": "@vercel/node" }]
    }
  ]
}
